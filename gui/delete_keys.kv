<DeleteKeysScreen>:
    id: main_win

    BoxLayout:
        orientation: 'vertical'

        Button:
            text: 'Back'
            size_hint: (1, 0.1)
            on_release: app.go_back()  # Assuming you have a method in your app class to handle the back action

        RecycleView:
            viewclass: 'CustomLabel'
            id: table_floor
            RecycleGridLayout:
                id: table_floor_layout
                cols: 5
                default_size: (None, 250)
                default_size_hint: (1, None)
                size_hint_y: None
                height: self.minimum_height
                spacing: 5

<CustomLabel>:
    bcolor: (1, 1, 1, 1)
    id: None
    name: None
    canvas.before:
        Color:
            rgba: root.bcolor
        Rectangle:
            size: self.size
            pos: self.pos
